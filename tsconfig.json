{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
        "@main*": [
          "./app/main*",
        ],
        "@components/*": [
          "./components/*"
        ],
        "@utils/*": [
          "./utils/*"
        ],
        "@assets/*": [
          "./assets/*"
        ],
        "@helper/*": [
          "./helper/*"
        ],
        "@services/*": [
          "./services/*"
        ],
        "@hooks/*": [
          "./hooks/*"
        ],
        "@lib/*": [
          "./lib/*"
        ],
        "@context/*": [
          "./context/*"
        ],
        "@api/*": [
          "./api/*"
        ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
, "app/main/_layout.jsx", "app/_layout.jsx", "app/+not-found.jsx"  ]
}